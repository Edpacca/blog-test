{
  "name": "gatsby-test",
  "private": true,
  "description": "A simple static site for Scott Logic CI/CD training",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
